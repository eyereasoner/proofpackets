@prefix : <http://example.org/#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix skolem: <https://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.

:pat a rdfs:Resource.
:bob :loves skolem:t_0.
skolem:t_0 :is :lonely.

#
# rdfproof
#

(_:bng_1 _:bng_2) log:proves _:bng_3.
(_:bng_4 _:bng_2) log:proves _:bng_5.

_:bng_1 {
    _:e_bng_2_1 log:isImpliedBy _:e_bng_3_1.
}

_:bng_2 {
    (:bob) log:skolem skolem:t_0.
}

_:bng_3 {
    :bob :loves skolem:t_0.
}

_:bng_4 {
    _:e_bng_4_1 log:isImpliedBy _:e_bng_5_1.
}

_:bng_5 {
    skolem:t_0 :is :lonely.
}

_:e_bng_2_1 {
    var:A :loves var:B.
}

_:e_bng_3_1 {
    (var:A) log:skolem var:B.
}

_:e_bng_4_1 {
    var:B :is :lonely.
}

_:e_bng_5_1 {
    (var:A) log:skolem var:B.
}
